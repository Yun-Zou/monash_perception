<launch>
    <!-- This node will rectify fisheye images obtained from the Realsense T265 camera (realsense-ros) -->

    <arg name="param_file_path" default="$(find monash_perception)/cfg/t265.yaml"/>
    <node pkg="monash_perception" name="t265_undistort" type="t265_undistort_node" output="screen">
        <param name="param_file_path" value="$(arg param_file_path)" /> 
    </node>
    
</launch>